  /**
   * Signs the user in.
   */
  Auth.signIn = () => {
    Auth.FB_AUTH.signInWithRedirect(Auth.FB_PROVIDER);
  };

  /**
   * Signs the user out.
   */
  Auth.signOut = () => {
    Auth.FB_AUTH.signOut();
