  /**
   * Handles clicks on the scroll trigger.
   * @param {Event} e The event returned from the handler.
   */
  handleClick_(e) {
    this.adapter_.preventDefault(e);
    const el = this.elementScrollTarget_;
    el && this.scrollProvider_.scrollToElement(el);
  }
}
