        this.drawersParent_.classList.add(className);
      },
      drawersHaveMaterialized: () => {
        return this.drawersParent_.classList.contains(VISIBLE_CLASS);
      },
      appendClassToggleButton: (className) => {
        if (!this.toggleBtn_) {
          return;
